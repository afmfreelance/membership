<h1>New constant</h1>

<% form_for(@constant) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :thevalue %><br />
    <%= f.text_field :thevalue %>
  </p>
	
	<p>
    <%= f.label :datatype %><br />
    <%= f.text_field :datatype %>
  </p
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', constants_path %>