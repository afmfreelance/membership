<h1>Editing payment</h1>

<% form_for(@payment) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :amount %><br />
    <%= f.text_field :amount %>
  </p>
  <p>
    <%= f.label :payment_type_id %><br />
    <%= f.text_field :payment_type_id %>
  </p>
  <p>
    <%= f.label :musician_id %><br />
    <%= f.text_field :musician_id %>
  </p>
  <p>
    <%= f.label :local_id %><br />
    <%= f.text_field :local_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @payment %> |
<%= link_to 'Back', payments_path %>